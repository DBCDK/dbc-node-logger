{
  "name": "dbc-node-logger",
  "version": "0.0.1",
  "description": "DBC logger module that ensures logging to central DBC syslog and New Relic",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register src/__tests__",
    "test:watch": "mocha --compilers js:babel/register src/__tests__ -w",
    "test:junit": "mocha --compilers js:babel/register src/__tests__ -R mocha-jenkins-reporter",
    "cleardist": "rm -rf ./dist",
    "build": "npm run cleardist && babel ./src -d dist --ignore __tests__",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "watch": "npm run cleardist && babel ./src --watch -d dist --ignore __tests__",
    "reinstall": "rm -r node_modules && npm install"
  },
  "keywords": [
    "DBC"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/dbc-node-logger"
  },
  "bugs": {
    "url": "https://github.com/DBCDK/dbc-node-logger/issues"
  },
  "author": "DBC A/S",
  "license": "ISC",
  "dependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.7.4",
    "babel-loader": "^5.3.2",
    "express-winston": "^0.3.1",
    "node-libs-browser": "^0.5.2",
    "webpack": "^1.10.1",
    "winston": "^1.0.1",
    "winston-syslog": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "chai": "^3.2.0",
    "eslint": "^1.0.0",
    "mocha": "^2.2.5",
    "mocha-jenkins-reporter": "^0.1.9",
    "sinon": "^1.15.4"
  }
}
